{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*).(jpg|jpeg|png|svg|ico|css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/favorites",
      "destination": "/favorites.html"
    },
    {
      "source": "/top100",
      "destination": "/top100.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    }
  ]
}